{
  "name": "pode",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern",
    "test": "jest",
    "demo": "cross-env REACT_APP_USE_DEMO_SERVICE=true expo start --clear"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-picker/picker": "^2.9.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "expo": "^52.0.11",
    "expo-av": "^15.0.2",
    "expo-splash-screen": "^0.29.9",
    "expo-status-bar": "~2.0.0",
    "formik": "^2.4.6",
    "query-string": "^9.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-dropdown-select-list": "^2.0.5",
    "react-native-picker-select": "^9.3.1",
    "react-native-web": "~0.19.10",
    "react-router-native": "^6.26.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/eslint-parser": "^7.25.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.2",
    "react-test-renderer": "^18.3.1"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "private": true
}
